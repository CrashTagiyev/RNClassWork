{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [
      "react",
      "react-native",
      "@react-navigation/native"
    ],
    "baseUrl": "./",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}